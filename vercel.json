{
  "version": 2,
  "builds": [
    {
      "src": "1.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "1.py"
    }
  ],
  "functions": {
    "api/hello.py": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}
